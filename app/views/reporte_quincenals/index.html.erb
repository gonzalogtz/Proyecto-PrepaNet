<p id="notice"><%= notice %></p>

<h1>Reporte Quincenals</h1>

<table>
  <thead>
    <tr>
      <th>Estatus</th>
      <th>Localizado</th>
      <th>Comentarios</th>
      <th>Tutor</th>
      <th>Alumno</th>
      <th>Fecha</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reporte_quincenals.each do |reporte_quincenal| %>
      <tr>
        <td><%= reporte_quincenal.estatus %></td>
        <td><%= reporte_quincenal.localizado %></td>
        <td><%= reporte_quincenal.comentarios %></td>
        <td><%= reporte_quincenal.tutor %></td>
        <td><%= reporte_quincenal.alumno %></td>
        <td><%= reporte_quincenal.fecha %></td>
        <td><%= link_to 'Show', reporte_quincenal %></td>
        <td><%= link_to 'Edit', edit_reporte_quincenal_path(reporte_quincenal) %></td>
        <td><%= link_to 'Destroy', reporte_quincenal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reporte Quincenal', new_reporte_quincenal_path %>
