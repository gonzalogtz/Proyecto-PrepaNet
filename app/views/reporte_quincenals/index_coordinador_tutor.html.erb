<%= render 'topsidebar/navsidebar' %>
  <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
    <div class="panel-heading">
      <h1 class="page-header">Reportes Quincenales</h1>
    </div>
    
    <% get_tutores_encargados.each do |tutor| %>
        <div>
            <h3 class='reportes_header' id='<%=tutor.cuenta%>'><span><img class='arrow_icon' src="expand_arrow.png"></span>Tutor: <%=tutor.nombres%> <%=tutor.apellido_p%> <%=tutor.apellido_m%></h3>
            <div class='reportes_content open padding_inside'>
                <%= render 'tarjetas', tutor: tutor.cuenta%>
            </div>
        </div>
    <% end %>
    
  </div>
</div>