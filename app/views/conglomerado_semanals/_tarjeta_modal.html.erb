<div class="tutor_tarjeta">
    <div class='tarjeta_texto_nombre'><%=@tutor.nombres + " " + @tutor.apellido_p + " " + @tutor.apellido_m%></div>
    <div class='tarjeta_matricula'><%=@tutor.nomina_matricula%> // <%=@tutor.cuenta%></div>
    <div class='tarjeta_matricula'><%=@curso.campus%></div>
    <div class='tarjeta_matricula'><%=@curso.materia%> - <%=@curso.grupo%></div>
    <div class='tarjeta_matricula'>Coordinador de tutores: <%=get_usuario_name_by_cuenta(@curso.coordinador_tutores)%></div>
    <div class='contenedor_boton_reportes'>
        <div class='contenedor_curso_reportes'>
            <div class='contenedor_boton_reportes_header'>Reporte</div>
            <%=get_conglomerado_semanals_button_by_tutor_and_curso(@tutor.cuenta, @curso.grupo)%>
        </div>
    </div>
</div>