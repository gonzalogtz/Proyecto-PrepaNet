<div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
    <div class="tarjeta tutor_tarjeta">
        <div class='tarjeta_nombre'><%=tutor.nombres%></div>
        <div class='tarjeta_apellidos'><%=tutor.apellido_p%> <%=tutor.apellido_m%></div>
        <div class='tarjeta_matricula'><%=tutor.cuenta%></div>
        <div class='contenedor_boton_reportes'>
            <% get_cursos_by_tutor(tutor.cuenta).each do |curso| %>
                <%get_reporte_semanals_by_tutor_and_curso(tutor.usuario, curso.curso)%>
                <div class='contenedor_boton_reportes_header'>Reportes - <%= curso.materia %></div>
                    <% (1..15).each do |i| %>
                        <%=get_reporte_semanals_by_semana(i)%>
                    <% end %>
            <% end %>
        </div>
    </div>
</div>