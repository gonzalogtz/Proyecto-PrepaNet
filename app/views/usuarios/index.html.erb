<%= render 'topsidebar/navsidebar' %>
<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
  <div id="table-cursos">
    <div class="panel panel-primary">

    <div class="panel-heading">
      <center><h1>Usuarios</h1></center>
    </div>

    <div class="table-responsive" style="">
        <table class="table table-striped txtCenter">
          <thead>
            <tr>
              <th class="txtCenter">Cuenta</th>
              <th class="txtCenter">Nomina/Matricula</th>
              <th class="txtCenter">Contrasena</th>
              <th class="txtCenter">Campus</th>
              <th class="txtCenter">Rol</th>
              <th class="txtCenter">Titulo</th>
              <th class="txtCenter">Nombres</th>
              <th class="txtCenter">Apellido Paterno</th>
              <th class="txtCenter">Apellido Materno</th>
              <th class="txtCenter">Correo</th>
              <th class="txtCenter">Telefono</th>
              <th class="txtCenter">Estatus</th>
            </tr>
          </thead>
          <tbody>
            <% @usuarios.each do |user| %>
              <tr>
                <td><%= user.cuenta %></td>
                <td><%= user.nomina_matricula %></td>
                <td><%= user.contrasena %></td>
                <td><%= user.campus %></td>
                <td><%= user.rol %></td>
                <td><%= user.titulo %></td>
                <td><%= user.nombres %></td>
                <td><%= user.apellido_p %></td>
                <td><%= user.apellido_m %></td>
                <td><%= user.correo %></td>
                <td><%= user.telefono %></td>
                <td><%= user.periodo %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>

      
    </div>
  </div>
</div>